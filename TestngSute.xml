<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Abhibus QA Automation">
	<listeners>
		<listener class-name="seleniumPack.MyListeners" />
	</listeners>
	<test name="Regression Test with chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="seleniumPack.AbhibusTest" />
		</classes>
	</test>

	<test name="Regression Test with firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="seleniumPack.AbhibusTest" />
		</classes>
	</test>
</suite>